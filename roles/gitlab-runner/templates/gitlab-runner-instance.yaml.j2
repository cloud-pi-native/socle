apiVersion: apps.gitlab.com/v1beta2
kind: Runner
metadata:
  name: gitlab-runner
  namespace: {{ GITLAB_NAMESPACE }}
spec:
  gitlabUrl: https://{{ GITLAB_DOMAIN }}/
  runUntagged: true
  buildImage: alpine
  token: gitlab-gitlab-runner-secret
  tags: openshift
  env: custom-env
{% if additionals_ca | length %}
  ca: ca-bundle-dso
{% endif %}
  #config: custom-config-toml
