apiVersion: postgresql.cnpg.io/v1
kind: Pooler
metadata:
  name: pooler-pg-cluster-gitlab-rw
  namespace: {{ dsc.gitlab.namespace }}
spec:
  cluster:
    name: pg-cluster-gitlab
  instances: 3
  type: rw
  pgbouncer:
    poolMode: session
    parameters:
      max_client_conn: "1000"
      default_pool_size: "100"
