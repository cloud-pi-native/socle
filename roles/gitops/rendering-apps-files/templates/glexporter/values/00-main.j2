glexporter:
  fullnameOverride: "glexporter"
  config:
    gitlab:
      url: http://gitlab-webservice-default.{{ dsc.gitlab.namespace }}.svc.cluster.local:8181/
      token: <path:{{ vaultinfra_kv_name }}/data/env/{{ dsc_name }}/apps/gitlab/values#gitlabToken>
    redis:
      url: "redis://glexporter-redis-master.{{ dsc.gitlab.namespace }}.svc:6379"
    wildcards:
      - {}
  serviceMonitor :
    enabled: true
    endpoints:
      - port: http
        interval: 30s
        path: /metrics
  redis:
    fullnameOverride: "glexporter-redis"
    global:
      imageRegistry: "<path:{{ vaultinfra_kv_name }}/data/env/{{ dsc_name }}/apps/global/values#image | jsonPath {.repository.docker}>"
    image:
      repository: bitnamilegacy/redis
