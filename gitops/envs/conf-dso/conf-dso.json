{
  "env": "conf-dso",
  "provider": "self-hosted",
  "region": "fr-par",
  "prefix": "dso-",
  "destination": {
    "clusterName": "in-cluster"
  },
  "targetRevision": "develop",
  "apps": [
    { "app": "keycloak", "enabled": "true", "namespace": "keycloak", "syncWave": 10 },
    { "app": "vault", "enabled": "true", "namespace": "vault", "syncWave": 10 },
    { "app": "sonarqube", "enabled": "true", "namespace": "sonarqube", "syncWave": 10 },
    { "app": "harbor", "enabled": "true", "namespace": "harbor", "syncWave": 10 },
    { "app": "observability", "enabled": "true", "namespace": "observability", "syncWave": 10 },
    { "app": "cert-manager", "enabled": "true", "namespace": "cert-manager", "syncWave": 10 }
  ]
}
