apiVersion: v1
kind: Service
metadata:
  namespace: {{ DSC.sonarqube.namespace }}
  name: sonar # Sets service name
  labels:
    app: sonar # Labels and Selectors
spec:
  type: ClusterIP # Sets service type
  ports:
    - name: http
      port: 9000
      protocol: TCP
  selector:
    app: sonar
